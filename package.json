{
	"name": "@worqhat/wizard",
	"version": "1.4.0",
	"description": "The WorqHat wizard helps you to configure your project",
	"private": false,
	"license": "MIT",
	"type": "commonjs",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"bin": {
		"worqhat-wizard": "dist/index.js"
	},
	"files": ["dist"],
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"postbuild": "node -e \"require('fs').chmodSync('dist/index.js', 0o755)\"",
		"clean": "rimraf dist",
		"dev": "tsx watch src/index.ts",
		"start": "node dist/index.js",
		"format": "biome format --write .",
		"lint": "biome lint .",
		"check": "biome check .",
		"prepare": "npm run build",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"keywords": ["cli", "wizard", "worqhat"],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/WorqHat/worqhat-wizard-cli.git"
	},
	"bugs": {
		"url": "https://github.com/WorqHat/worqhat-wizard-cli/issues"
	},
	"homepage": "https://github.com/WorqHat/worqhat-wizard-cli#readme",
	"author": "WorqHat <hello@worqhat.com> (https://worqhat.com)",
	"contributors": [
		"Sagnik Ghosh <sagnik@worqhat.com>"
	],
	"engines": {
		"node": ">=18"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"chalk": "^4.1.2",
		"enquirer": "^2.4.1",
		"undici": "^6.19.8"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.3",
		"@types/node": "^22.5.4",
		"rimraf": "^6.0.1",
		"ts-node": "^10.9.2",
		"tslib": "^2.7.0",
		"tsup": "^8.2.4",
		"tsx": "^4.19.2",
		"typescript": "^5.5.4"
	}
}
